@using System.Security.Claims
@{
    ViewData["Title"] = "User`s claims";
}

@model IList<Claim>

@{
    if (Model is not null)
    {
        for (int i = 0; i < Model.Count; i++)
        {
            <h5>@Model[i].Type - @Model[i].Value</h5>
        }
    }
    else
    {
        <h2>Empty claims data</h2>
    }
}
